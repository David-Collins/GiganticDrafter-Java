<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="400.0" spacing="10.0" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="25.0">
         <children>
            <Label text="Choose from list of servers or enter a unique server name and hit create server here" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="serverName" promptText="Enter a unique server name here" />
            <Button fx:id="createServer" mnemonicParsing="false" text="Create Server" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="List of Current Servers" textAlignment="CENTER">
               <font>
                  <Font size="29.0" />
               </font>
            </Text>
            <ScrollPane prefHeight="415.0" prefWidth="393.0">
               <content>
                  <VBox fx:id="serverList" alignment="TOP_CENTER" prefHeight="410.0" prefWidth="380.0" />
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
